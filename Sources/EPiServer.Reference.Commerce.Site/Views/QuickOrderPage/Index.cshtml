@model EPiServer.Reference.Commerce.Site.Features.QuickOrder.ViewModels.QuickOrderPageViewModel

<div class="container-fluid">
    <div class="row myPage">
        @Html.Partial("_CmsNavigation", Model.CurrentPage)
        <main class="col-xs-12 col-sm-9">
            <div class="spacer-left-l">
                <section>
                    <div class="section-title spacer-bottom-l">
                        <h2 class="h3">Quick Order Interface</h2>
                    </div>

                    <div class="row">
                        <div class="col-sm-4">
                            <label>Product name</label>
                        </div>
                        <div class="col-sm-2">
                            <label>SKU</label>
                        </div>
                        <div class="col-sm-2">
                            <label>Unit Price</label>
                        </div>
                        <div class="col-sm-2">
                            <label>Quantity</label>
                        </div>
                        <div class="col-sm-2">
                            <label>Total Price</label>
                        </div>
                    </div>
                    <form id="quickOrderForm">
                        <div class="row order-row">
                            <div class="form-group col-sm-4">
                                <input type="text" id="" name="productname1" placeholder="" required/>
                            </div>
                            <div class="form-group col-sm-2">
                                <input type="text" id="" name="sku1" placeholder="" required/>
                            </div>
                            <div class="form-group col-sm-2">
                                <input type="text" id="" name="unitprice1" placeholder="" required/>
                            </div>
                            <div class="form-group col-sm-2">
                                <input type="text" id="" name="quantity1" placeholder="" required/>
                            </div>
                            <div class="form-group col-sm-2">
                                <input type="text" id="" name="totalprice1" placeholder="" required/>
                            </div>
                            <a href="#" class="btn btn-danger btn-xs delete-icon">
                                <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
                            </a>
                        </div>
                        <div class="row order-row">
                            <div class="form-group col-sm-4">
                                <input type="text" id="" name="productname2" placeholder="" required />
                            </div>
                            <div class="form-group col-sm-2">
                                <input type="text" id="" name="sku2" placeholder="" required />
                            </div>
                            <div class="form-group col-sm-2">
                                <input type="text" id="" name="unitprice2" placeholder="" required />
                            </div>
                            <div class="form-group col-sm-2">
                                <input type="text" id="" name="quantity2" placeholder="" required />
                            </div>
                            <div class="form-group col-sm-2">
                                <input type="text" id="" name="totalprice2" placeholder="" required />
                            </div>
                            <a href="#" class="btn btn-danger btn-xs delete-icon">
                                <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
                            </a>
                        </div>
                        <a class="btn btn-sm btn-primary js-add-row-btn">Add row</a>
                        <div class="spacer-top-l">
                            <input type="file" data-interface="lattice" data-label="Upload CVS">

                            <button type="submit" class="btn btn-primary">Import</button>
                            <a href="@Url.Action("Index", "UsersPage")" class="btn btn-default">Cancel</a>
                        </div>
                    </form>
                </section>
            </div>
        </main>
    </div>
</div>